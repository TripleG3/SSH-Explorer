{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET MAUI",
            "type": "maui",
            "request": "launch",
            "preLaunchTask": "maui: Build"
        },
        {
            "name": "C#: SSHExplorer [Local Machine]",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}\\SSHExplorer.csproj",
            "launchConfigurationId": "windowsmachine"
        },
        {
            "name": ".NET MAUI (Windows) - Debug (no build)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "",
            "program": "${workspaceFolder}\\bin\\Debug\\net9.0-windows10.0.19041.0\\win10-x64\\SSHExplorer.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "DOTNET_ENVIRONMENT": "Development"
            }
        }
    ]
}